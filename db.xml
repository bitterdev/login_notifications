<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="UserLoginNotifications">
        <field name="id" type="integer">
            <unsigned/>
            <autoincrement/>
            <key/>
        </field>
        <field name="fingerprint" type="string"/>
        <field name="userAgent" type="string"/>
        <field name="uID" type="integer">
            <unsigned/>
        </field>
        <field name="ip" type="string"/>
        <field name="loginAt" type="datetime"/>
    </table>
</schema>
